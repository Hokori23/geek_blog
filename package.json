{
  "name": "geek_blog",
  "version": "1.0.0",
  "description": "Geek blog ，大体技术栈：nuxt.js + express",
  "author": "Hokori",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "cd _server",
    "server:install": "cd _server && npm i -f",
    "server:build": "cd _server && tsc || npm run ?server:build",
    "?server:build": "echo Error: \"编译.ts文件需要全局安装typescript, 请执行命令npm i -g typescript\"",
    "server:install:ts": "cd_server && npm i -g typescript",
    "client": "cd _client && npm run dev",
    "client:install": "cd _client && npm i -f"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hokori23/geek_blog.git"
  },
  "keywords": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Hokori23/geek_blog/issues"
  },
  "homepage": "https://github.com/Hokori23/geek_blog#readme",
  "dependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "husky": "^4.2.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}